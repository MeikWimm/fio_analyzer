<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.atool.PrimaryController">
   <top>
      <MenuBar fx:id="menuBar" prefHeight="22.0" prefWidth="666.0" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="menuItem_open" mnemonicParsing="false" onAction="#openLogfile" text="Open" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Tests">
            <items>
              <MenuItem fx:id="menuItem_ANOVA" mnemonicParsing="false" onAction="#executeANOVA" text="ANOVA" />
                  <MenuItem mnemonicParsing="false" text="Confidence Interval" />
                  <MenuItem mnemonicParsing="false" text="T-Test" />
                  <MenuItem mnemonicParsing="false" text="Tukey-HSD" />
                  <MenuItem mnemonicParsing="false" text="U-Test" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
                  <MenuItem fx:id="menuItem_Info" mnemonicParsing="false" onAction="#openInfoWindow" text="Info" />
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <Button fx:id="primaryButton" mnemonicParsing="false" onAction="#switchToSecondary" text="Button" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TableView id="table" fx:id="table" editable="true" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn id="fileNameColumn" fx:id="fileNameColumn" prefWidth="107.0" text="File" />
                <TableColumn id="runsColumn" fx:id="runsColumn" prefWidth="43.0" text="Runs" />
                  <TableColumn id="speedColumn" fx:id="speedColumn" prefWidth="101.0" text="Average Speed" />
                  <TableColumn id="timeColumn" fx:id="timeColumn" prefWidth="90.0" text="Time (in millisec)" />
                  <TableColumn id="lastModifiedColumn" fx:id="lastModifiedColumn" prefWidth="130.0" text="Last Modified" />
                  <TableColumn id="fileCreatedColumn" fx:id="fileCreatedColumn" prefWidth="128.0" text="File Created" />
                  <TableColumn id="fileCreatedColumn" fx:id="epsilonColumn" prefWidth="128.0" text="Epsilon for RDP" />
              </columns>
            </TableView>
         </children></VBox>
   </center>
</BorderPane>
